function 'Jespervb7.PercentDifference' =
		(
		            pCurrentValue : SCALAR,
		            pPreviousValue : SCALAR
		        ) =>
		        VAR _NewMinusOld = pCurrentValue - pPreviousValue
		        VAR _Result =
		            DIVIDE (
		                _NewMinusOld,
		                pPreviousValue
		            )
		        RETURN
		            _Result
