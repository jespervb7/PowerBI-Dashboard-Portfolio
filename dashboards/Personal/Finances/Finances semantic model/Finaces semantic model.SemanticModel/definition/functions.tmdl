		    VAR _Prefix =
		        IF ( _CurrencyPosition = "PREFIX", _CurrencySymbol & " ", "" )
		
		    VAR _Suffix =
		        IF ( _CurrencyPosition = "SUFFIX", " " & _CurrencySymbol, "" )
		
		    VAR _Format =
		        _Prefix & _BaseFormat & _Suffix
		
		    RETURN
